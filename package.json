    "@tanstack/react-query": "^5.83.0",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.525.0",
    "postcss": "^8.5.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.61.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.7.1",
    "sonner": "^2.0.6",
